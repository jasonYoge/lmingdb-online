{
    "envId": "lmingdb-2gfglj1s626cc70f",
    "version": "2.0",
    "$schema": "https://framework-1258016615.tcloudbaseapp.com/schema/latest.json",
    "functionRoot": "./functions",
    "functions": [],
    "framework": {
        "name": "lmingdb",
        "plugins": {
            "website": {
                "use": "@cloudbase/framework-plugin-website",
                "inputs": {
                    "outputPath": "./dist"
                }
            }
        }
    },
    "route": [
        {
            "path": "/search",
            "method": "GET",
            "handler": {
                "type": "static",
                "source": "./dist/index.html"
            }
        },
        {
            "path": "/upload",
            "method": "GET",
            "handler": {
                "type": "static",
                "source": "./dist/index.html"
            }
        },
        {
            "path": "/**",
            "method": "GET",
            "handler": {
                "type": "static",
                "source": "path/to/your/static/files"
            }
        }
    ]
}
